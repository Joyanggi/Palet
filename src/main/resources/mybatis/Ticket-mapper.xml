<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="ticket">
  	<select id="myticket" resultType="Trillion.Palet.DTO.TicketDTO">
  		select * from myticket where email=#{email}
  	</select>
  	  	<select id="isuseticket" resultType="Trillion.Palet.DTO.TicketDTO">
  		select * from myticket where et_booknumber=#{et_booknumber}
  	</select>
  </mapper>